{
    "name": "react-subscribe-context",
    "version": "0.1.0",
    "private": false,
    "author": "Akia Vongdara",
    "description": "Simple state management hooks that works like React Context but optmized to render in components only where the data it's subscribed to changes.",
    "license": "MIT",
    "source": "src/react-subscribe-context/index.ts",
    "main": "dist/index.js",
    "module": "dist/module.js",
    "types": "dist/types.d.ts",
    "files": [
        "dist/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/vongdarakia/react-subscribe-context"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build-lib": "parcel build --reporter @parcel/reporter-bundle-buddy"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "ie 11",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.3.2",
        "@parcel/transformer-typescript-types": "^2.3.2",
        "@swc/helpers": "^0.3.5",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@testing-library/user-event": "^13.5.0",
        "@types/events": "^3.0.0",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.25",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/styled-components": "^5.1.23",
        "@welldone-software/why-did-you-render": "^6.2.3",
        "axios": "^0.26.0",
        "deep-proxy-polyfill": "^1.0.4",
        "parcel": "^2.3.2",
        "react": "^17.0.2",
        "react-app-polyfill": "^3.0.0",
        "react-dom": "^17.0.2",
        "react-scripts": "^5.0.0",
        "react-tracked": "^1.7.6",
        "scheduler": "^0.20.2",
        "styled-components": "^5.3.3",
        "typescript": "^4.5.5",
        "web-vitals": "^2.1.4"
    },
    "peerDependencies": {
        "@swc/helpers": "^0.3.5",
        "deep-proxy-polyfill": "^1.0.4",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "alias": {
        "components/*": "./src/components/$1",
        "constants/*": "./src/constants/$1",
        "definitions/*": "./src/types/$1",
        "examples/*": "./src/examples/$1",
        "react-subscribe-context/*": "./src/react-subscribe-context/$1",
        "utils/*": "./src/utils/$1"
    }
}
